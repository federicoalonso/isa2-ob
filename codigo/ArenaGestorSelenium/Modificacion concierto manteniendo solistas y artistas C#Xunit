      IReadOnlyCollection<IWebElement> elements = driver.FindElements(By.CssSelector(".selected-item > span"));
      Assert.True(elements.Count > 0);
    }
    {
      IReadOnlyCollection<IWebElement> elements = driver.FindElements(By.CssSelector(".mb-3:nth-child(5) .selected-item > span"));
      Assert.True(elements.Count > 0);
    }
    driver.FindElement(By.LinkText("Volver")).Click();
    driver.FindElement(By.Name("filterEndDate")).Click();
    driver.FindElement(By.XPath("//span[contains(.,\'2023\')]")).Click();
